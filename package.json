  },
  "homepage": "https://github.com/zerotacg/nel-io#readme",
  "devDependencies": {
    "babel": "^5.8.24",
    "chai": "^3.4.1",
    "jshint": "^2.8.0",
    "jspm": "^0.16.15",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
